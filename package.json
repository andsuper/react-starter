{
  "name": "react-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "develop": "next",
    "build": "next build",
    "format": "prettier --write '**/*.{ts,tsx}'",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "dependencies": {
    "next": "^9.0.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "babel-plugin-styled-components": "^1.10.6",
    "husky": "^3.0.4",
    "prettier": "^1.18.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  }
}
